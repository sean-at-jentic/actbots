# ActBots Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Jentic Platform Configuration
JENTIC_API_KEY=your_jentic_api_key_here

# Language Model Configuration
# Choose one of the following models:
# - OpenAI models: gpt-4o, gpt-4, gpt-3.5-turbo
# - Gemini models: gemini/gemini-pro, gemini/gemini-1.5-pro
LLM_MODEL=gpt-4o

# OpenAI API Key (required if using OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Gemini API Key (required if using Gemini models)
# GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
